<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>
    <p>Welcome to <%= title %></p>
    <% if (messages.length !== 0) { %>
      <% messages.forEach((message) => { %>  
        <h4><%= message.text %></h4>
        <p><%= message.user %> </p>
      <% }) %>
    <% } %>  
  </body>
</html>
